@model List<MovieModel>

@{
    ViewData["Title"] = "Liste des films";
}

<h1>Ici la liste de vos films préférés</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Année de sortie</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var Movie in Model)
        {
            <tr>
                <td>@Movie.Id</td>
                <td>@Movie.Name</td>
                <td>@Movie.ReleaseDate</td>
            </tr>
        }      
    </tbody>
</table>


